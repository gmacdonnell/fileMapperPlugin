
  CREATE TABLE "I2B2DEMODATA"."FILE_MAP_ELEMENTS" 
   (	"MAP_ID" NUMBER NOT NULL ENABLE, 
	"SOURCE" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"ELEMENT_ID" NUMBER NOT NULL ENABLE, 
	"COLUMN_INDEX" NUMBER NOT NULL ENABLE, 
	"ACTIVE" VARCHAR2(1 BYTE) DEFAULT 'Y' NOT NULL ENABLE, 
	"TARGET_TABLE" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"TARGET_FIELD" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"NAME_ON_SOURCE" VARCHAR2(50 BYTE), 
	"IS_FACT" VARCHAR2(1 BYTE) NOT NULL ENABLE, 
	"CONCEPT_CD" VARCHAR2(50 BYTE), 
	"IS_KEY" VARCHAR2(1 BYTE) NOT NULL ENABLE, 
	"TRANSFORMATION" VARCHAR2(50 BYTE) DEFAULT 'NONE' NOT NULL ENABLE, 
	"IS_VIRTUAL_COLUMN" VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL ENABLE, 
	"VIRTUAL_VALUE" VARCHAR2(50 BYTE)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS" ;

  CREATE INDEX "I2B2DEMODATA"."FILE_MAP_ELEMENTS_INDEX1" ON "I2B2DEMODATA"."FILE_MAP_ELEMENTS" ("MAP_ID", "SOURCE", "ELEMENT_ID", "COLUMN_INDEX", "ACTIVE") 
  PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS" ;
